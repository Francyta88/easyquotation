<core:FragmentDefinition xmlns:t="sap.ui.table" xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core" xmlns="sap.m"
	height="100%">
	<Dialog horizontalScrolling="false" showHeader="true" state="None" stretch="false" title="{i18n>SMAfastEntry}" type="Standard"
		verticalScrolling="true" class="sapUiResponsiveContentPadding" contentHeight="70%" contentWidth="90%">
		<content>
			<t:Table id="TableSearchMaterialArea" rows="{customSearchArea>/}" selectionMode="None" visibleRowCount="7" rowHeight="50%">
				<t:extension>
					<OverflowToolbar>
						<ToolbarSpacer/>
						<u:FileUploader buttonText="{i18n>SMAuploadFIle}" id="FileUploaderId" sameFilenameAllowed="true" iconOnly="false" buttonOnly="true"
							fileType="XLSX,xlsx" icon="sap-icon://upload" iconFirst="true" style="Emphasized" change="onUploadSMA" class="marginUploadFile"/>
						<Button id="downloadButton" text="{i18n>SMAdownloadTemplate}" press="downloadTemplate"/>
					</OverflowToolbar>
				</t:extension>
				<t:columns>
					<t:Column width="4rem" hAlign="Center">
						<Label text=""/>
						<t:template>
							<Button icon="sap-icon://delete" press="deleteItemSMA"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAmaterialCode}"/>
						<t:template>
							<Input value="{customSearchArea>Material}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAcustomerMaterial}"/>
						<t:template>
							<Input value="{customSearchArea>CustomerMaterial}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAplant}"/>
						<t:template>
							<Input value="{customSearchArea>Plant}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAfactor}"/>
						<t:template>
							<Input value="{customSearchArea>Factor}" class="customCell" type="Number"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAlength}"/>
						<t:template>
							<Input value="{customSearchArea>Length}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAconditionType}"/>
						<t:template>
							<Input value="{customSearchArea>PriceCondition}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAamount}"/>
						<t:template>
							<Input value="{customSearchArea>ConditionValue}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAper}"/>
						<t:template>
							<Input value="{customSearchArea>ConditionLength}" class="customCell" type="Number"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAuom}"/>
						<t:template>
							<Input value="{customSearchArea>MeasureUnit}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAcm}"/>
						<t:template>
							<Input value="{customSearchArea>Cm}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMArequestedDate}"/>
						<t:template>
							<Input value="{customSearchArea>RequestedDate}" class="customCell"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAitemNumber}"/>
						<t:template>
							<Input value="{customSearchArea>ItemNumber}" class="customCell" type="Number"/>
						</t:template>
					</t:Column>
					<t:Column width="9rem">
						<Label text="{i18n>SMAcustomerLength}"/>
						<t:template>
							<Input value="{customSearchArea>ZCustLength}" class="customCell"/>
						</t:template>
					</t:Column>
				</t:columns>
			</t:Table>
		</content>
		<buttons>
			<Button icon="sap-icon://cart-4" text="{i18n>SMAaddToCart}" press="addcartSMA"/>
			<Button icon="sap-icon://decline" text="{i18n>SMAcloseSearchMaterialArea}" press="closeSMA"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>